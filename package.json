{
  "name": "futuristic-kami-omni-engine",
  "version": "1.0.0",
  "description": "Multi-chain profit optimization engine with MCP server deployment capabilities",
  "main": "index.js",
  "scripts": {
    "start": "node mcp-server.js",
    "server:start": "node mcp-server.js",
    "azure-server:start": "node azure-mcp-server.js",
    "dashboard:start": "node empire-dashboard.js start",
    "dashboard:report": "node empire-dashboard.js report",
    "deploy:dashboard": "node dashboard-mainnet-deploy.js deploy",
    "deploy:mainnet": "node dashboard-mainnet-deploy.js mainnet",
    "build:dashboard": "cd dashboard && npm run build",
    "start:dashboard": "cd dashboard && npm start",
    "health:check": "node dashboard-mainnet-deploy.js health",
    "mcp:start-all": "npm run server:start & npm run azure-server:start",
    "azure:mutate": "node azure-key-provisioner.js execute",
    "azure:provision": "node azure-key-provisioner.js mutate",
    "azure:check-key": "node azure-key-provisioner.js check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js --network skale"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "axios": "^1.6.2",
    "@azure/identity": "^3.4.0",
    "@azure/storage-blob": "^12.17.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/arm-appservice": "^13.0.0",
    "@azure/arm-monitor": "^7.0.0",
    "@azure/arm-resources": "^5.2.0",
    "@azure/arm-keyvault": "^3.0.0",
    "@azure/arm-storage": "^18.1.0",
    "@azure/arm-cosmosdb": "^15.4.0",
    "openai": "^4.24.1",
    "dotenv": "^16.3.1",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.0"
